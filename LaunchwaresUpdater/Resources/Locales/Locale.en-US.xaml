<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:LaunchwaresUpdater.Resources.Locales"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">
    <system:String x:Key="application.title">Server Name</system:String>
    <system:String x:Key="application.starting">Program will update soon please wait</system:String>
    <system:String x:Key="application.downloadingdatas">Downloading datas hold on!</system:String>
    <system:String x:Key="application.downloadingfiles">Downloading Files %percentage%% (%downloaded%/%total%)</system:String>
    <system:String x:Key="application.finished">All files downloaded, program closing</system:String>
    <system:String x:Key="application.noupdate">The update files were not found. It may not be ready yet or your setup may be corrupted. Try again in half an hour and if you get the same error, please contact your authorized person.</system:String>
</ResourceDictionary>